<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">

    <item android:state_enabled="false">
        <shape>
            <corners android:radius="@dimen/sc_mm_22" />
