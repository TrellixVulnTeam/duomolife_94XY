<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white"
    android:layout_height="@dimen/sc_mm_200">
    <CheckBox
        android:layout_width="@dimen/sc_mm_30"
        android:id="@+id/cb_browse_history_header"
        android:button="@drawable/sel_brows_history_check"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_height="@dimen/sc_mm_30" />
    <TextView
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/cb_browse_history_header"
        android:layout_height="wrap_content"
        android:id="@+id/tv_browse_history_header"/>
</android.support.constraint.ConstraintLayout>