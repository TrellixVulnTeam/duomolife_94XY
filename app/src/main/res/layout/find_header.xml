<?xml version="1.0" encoding="utf-8"?>
<com.amkj.dmsh.views.autoviews.AutoToolbar xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/tl_find_header"
    android:background="@drawable/sel_bg_border_gray_white"
    app:layout_collapseMode="pin"
    android:minHeight="88px"
    app:contentInsetStart="0dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="88px"
        android:gravity="center_vertical">
        <TextView
            android:layout_width="wrap_content"
            android:textColor="@color/text_black_t"
            android:textSize="34px"
            android:text="发 现"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:layout_height="match_parent" />
        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_marginRight="18px"
            android:layout_marginLeft="31px"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:id="@+id/fra_find_message_total"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_width="64px"
                android:id="@+id/iv_find_message_total"
                android:layout_height="54px"
                android:paddingRight="12px"
                android:paddingTop="12px"
                android:src="@drawable/home_mes_icon"/>
        </FrameLayout>
    </RelativeLayout>

</com.amkj.dmsh.views.autoviews.AutoToolbar>