<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tl="http://schemas.android.com/apk/res-auto"
    android:id="@+id/fl_quality_bar"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:id="@+id/rl_toolbar2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/ll_back2"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/sc_mm_88"
            android:layout_alignParentLeft="true"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/sc_mm_20"
            android:paddingRight="@dimen/sc_mm_50">

            <ImageView
                android:id="@+id/iv_life_back2"
                android:layout_width="@dimen/sc_mm_60"
                android:layout_height="@dimen/sc_mm_60"
                android:src="@drawable/transparent_back"
                android:textColor="@color/home_text_color"
                android:textSize="@dimen/sc_mm_32" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_service2"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/sc_mm_88"
            android:layout_alignParentRight="true"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/sc_mm_20"
            android:paddingRight="@dimen/sc_mm_20">

            <ImageView
                android:id="@+id/iv_img_service2"
                android:layout_width="@dimen/sc_mm_60"
                android:layout_height="@dimen/sc_mm_60"
                android:src="@drawable/transparent_shopcar" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_share2"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/sc_mm_88"
            android:layout_toLeftOf="@+id/ll_service2"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/sc_mm_20">

            <ImageView
                android:id="@+id/iv_img_share2"
                android:layout_width="@dimen/sc_mm_60"
                android:layout_height="@dimen/sc_mm_60"
                android:src="@drawable/transparent_share" />
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:alpha="0"
        android:background="@drawable/border_bottom_line"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/ll_back"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/sc_mm_88"
            android:layout_alignParentLeft="true"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/sc_mm_36"
            android:paddingRight="@dimen/sc_mm_50">

            <ImageView
                android:id="@+id/iv_life_back"
                android:layout_width="@dimen/sc_mm_18"
                android:layout_height="@dimen/sc_mm_32"
                android:src="@drawable/common_back_btn_nor"
                android:textColor="@color/home_text_color"
                android:textSize="@dimen/sc_mm_32" />
        </LinearLayout>

        <com.amkj.dmsh.views.flycoTablayout.CommonTabLayout
            android:id="@+id/ctb_qt_pro_details"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/sc_mm_88"
            android:layout_centerHorizontal="true"
            tl:tl_indicator_color="@color/text_black_t"
            tl:tl_indicator_corner_radius="@dimen/dimen_1dp"
            tl:tl_indicator_height="@dimen/dimen_2dp"
            tl:tl_indicator_margin_bottom="@dimen/dimen_2dp"
            tl:tl_indicator_style="NORMAL"
            tl:tl_indicator_width="@dimen/dimen_20_dp"
            tl:tl_tab_padding="25dp"
            tl:tl_textSelectColor="@color/text_black_t"
            tl:tl_textUnselectColor="@color/text_black_t"
            tl:tl_textsize="@dimen/sc_mm_28" />

        <LinearLayout
            android:id="@+id/ll_service"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/sc_mm_88"
            android:layout_alignParentRight="true"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/sc_mm_20"
            android:paddingRight="@dimen/sc_mm_32">

            <ImageView
                android:id="@+id/iv_img_service"
                android:layout_width="@dimen/sc_mm_36"
                android:layout_height="@dimen/sc_mm_32"
                android:src="@drawable/shop_car_gray_icon" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_share"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/sc_mm_88"
            android:layout_toLeftOf="@+id/ll_service"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/sc_mm_20"
            android:paddingRight="@dimen/sc_mm_24">

            <ImageView
                android:id="@+id/iv_img_share"
                android:layout_width="@dimen/sc_mm_36"
                android:layout_height="@dimen/sc_mm_32"
                android:src="@drawable/common_share_btn_nor" />
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/fl_header_service"
        android:layout_width="@dimen/sc_mm_60"
        android:layout_height="@dimen/sc_mm_88"
        android:layout_gravity="right|top" />
</FrameLayout>